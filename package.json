{
  "name": "filebounce-webui",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build:dev": "npm run gen-protobuf && webpack --config config/webpack.dev.js --progress --profile",
    "server:dev": "npm run gen-protobuf && webpack-dev-server --config config/webpack.dev.js --progress --profile --watch --content-base src/",
    "server:prod": "npm run gen-protobuf && webpack-dev-server --config config/webpack.prod.js --progress --profile --watch --content-base src/",
    "server": "npm run server:dev",
    "start": "npm run server:dev",

    "protobuf:js": "pbjs -t static-module -w commonjs -o src/filebounce/protobufs/protobufs.generated.js src/filebounce/protobufs/*.proto",
    "protobuf:ts": "pbts -o src/filebounce/protobufs/protobufs.generated.d.ts src/filebounce/protobufs/protobufs.generated.js",
    "gen-protobuf": "npm run protobuf:js && npm run protobuf:ts",

    "postinstall": "npm run gen-protobuf"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fsufitch/filebounce-webui.git"
